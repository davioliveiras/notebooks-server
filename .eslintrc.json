{
    "env": {
        "node": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "eol-last": ["error","always"],
        "no-multiple-empty-lines": ["error", { "max": 1 }]
    }
}
